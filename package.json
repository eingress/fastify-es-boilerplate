{
  "name": "fastify-es2018-boilerplate",
  "version": "0.1.0",
  "description": "A minimalist Fastify ES2018 boilerplate with babel, eslint, jest, nodemon, and prettier.",
  "repository": "https://github.com/eingress/node-es2018-boilerplate.git",
  "author": "Scott Mathieson <scott@eingress.io>",
  "license": "ISC",
  "keywords": [
    "babel",
    "eslint",
    "fastify",
    "jest",
    "node",
    "prettier",
    "supertest"
  ],
  "scripts": {
    "build": "yarn build:clean && babel src -d dist",
    "build:clean": "rimraf dist/*",
    "build:watch": "yarn build -w",
    "coverage": "jest --collectCoverageFrom=src/**/*.js --coverage",
    "lint": "eslint './{,{__tests__,src}/**/}*.js' --fix --quiet",
    "prestart": "yarn build",
    "start": "node dist/main.js",
    "start:dev": "NODE_ENV=development nodemon src/main.js",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch"
  },
  "dependencies": {
    "async-exit-hook": "eingress/async-exit-hook#2f681fb",
    "fastify": "3.27.1",
    "fastify-autoload": "3.11.0",
    "fastify-plugin": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "7.17.0",
    "@babel/node": "7.16.8",
    "@babel/preset-env": "7.16.11",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.2"
  }
}
